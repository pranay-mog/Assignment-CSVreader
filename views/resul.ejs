<!DOCTYPE html>
<html>
  <head>
    <title>Transactions</title>
  </head>
  <body>
    <h1>Transactions</h1>
    <table>
      <thead>
        <tr>
          <th>Date</th>
          <th>Description</th>
          <th>Amount</th>
          <th>Currency</th>
        </tr>
      </thead>
      <tbody>
        <% transactions.forEach(function(transaction) { %>
        <tr>
          <td><%= transaction.date %></td>
          <td><%= transaction.description %></td>
          <td><%= transaction.amount %></td>
          <td><%= transaction.currency %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
